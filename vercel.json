{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/manageExpiredBatches.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/manageExpiredBatches",
      "dest": "api/manageExpiredBatches.js"
    }
  ],
  "functions": {
    "api/manageExpiredBatches.js": {
      "schedule": "13 16 * * *",
      "timezone": "Asia/Kolkata"
    }
  }
}
