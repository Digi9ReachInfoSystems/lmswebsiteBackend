{
  "version": 3,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/manageExpiredBatches.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/manageExpiredBatches",
      "dest": "api/manageExpiredBatches.js"
    }
  ],

   
      "crons": [
        {
          "path": "api/manageExpiredBatches.js",
          "schedule": "20 16 * * *"
        }
      ]
    
  
}
